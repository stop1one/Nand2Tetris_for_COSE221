CHIP fourAdder{
    IN a0, a1, a2, a3, b0, b1, b2, b3, c;
    OUT s0, s1, s2, s3, carry;

    PARTS:
    FullAdder(a=a0, b=b0, c=c, sum=s0, carry=c1);
    FullAdder(a=a1, b=b1, c=c1, sum=s1, carry=c2);
    FullAdder(a=a2, b=b2, c=c2, sum=s2, carry=c3);
    FullAdder(a=a3, b=b3, c=c3, sum=s3, carry=carry);
}