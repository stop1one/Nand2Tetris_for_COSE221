CHIP FullAdder4bit{
    IN a[4], b[4], c;
    OUT s[4], carry;

    PARTS:
    FullAdder(a=a[0], b=b[0], c=c, sum=s[0], carry=c1);
    FullAdder(a=a[1], b=b[1], c=c1, sum=s[1], carry=c2);
    FullAdder(a=a[2], b=b[2], c=c2, sum=s[2], carry=c3);
    FullAdder(a=a[3], b=b[3], c=c3, sum=s[3], carry=carry);
}